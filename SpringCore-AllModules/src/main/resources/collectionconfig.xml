       xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean class="org.springcore.collection.Emp" name="emp1">
        <property name="name" value="Yash"/>
        <property name="phones">
            <list>
                <value>23456789</value>
                <value>2345622789</value>
                <value>2345678222229</value>
                <null/>
            </list>
        </property>

        <property name="address">
            <set>
                <value>Ahmedabad</value>
                <value>Delhi</value>
                <value>Goa</value>
            </set>
        </property>

        <property name="courses">
            <map>
                <entry key="java" value="2months"></entry>
                <entry key="python" value="2months"></entry>

            </map>
        </property>

